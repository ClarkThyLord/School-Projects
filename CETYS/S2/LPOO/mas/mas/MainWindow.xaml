<Window x:Class="mas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mas"
        mc:Ignorable="d"
        Title="10 más con más!" ResizeMode="NoResize" Width="775" Height="550">
    <Grid>
        <TextBox Name="input" HorizontalAlignment="Left" Height="19" Margin="1,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="564"/>
        <ComboBox Name="PROBLEMS" HorizontalAlignment="Left" Margin="565,-1,0,0" VerticalAlignment="Top" Width="120" Height="20"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="685,-1,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox Name="output" HorizontalAlignment="Left" Height="470" Margin="10,24,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="740"/>
    </Grid>
</Window>
